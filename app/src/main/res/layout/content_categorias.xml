<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/content_categorias"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"


    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="mx.uxie.app.uxie.CategoriasActivity"
    tools:showIn="@layout/app_bar_categorias"
    android:background="@color/white">

    <LinearLayout
        android:layout_marginTop="-55dp"
        android:layout_width="match_parent"
        android:background="@mipmap/pensiones"
        android:layout_height="85dp"></LinearLayout>

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentStart="true">

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1">

            <RelativeLayout
                android:id="@+id/rl_category_a1"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1">

                <TextView
                    android:text="Automotriz"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/textView"
                    android:textColor="@color/md_black_1000"
                    android:layout_centerHorizontal="true"
                    android:layout_below="@+id/imageView4" />

                <ImageView
                    android:layout_height="64dp"
                    app:srcCompat="@drawable/ic_directions_car_white_24px"
                    android:id="@+id/imageView4"
                    android:layout_width="64dp"
                    android:layout_centerHorizontal="true"
                    android:layout_centerVertical="true"
                    tools:ignore="VectorDrawableCompat" />

                <ImageView
                    android:layout_height="match_parent"
                    app:srcCompat="@color/colorAccent"
                    android:id="@+id/imageView3"
                    android:layout_alignParentRight="true"
                    android:layout_width="1dp" />

                <ImageView
                    android:layout_width="match_parent"
                    app:srcCompat="@color/colorAccent"
                    android:id="@+id/imageView2"
                    android:layout_alignParentBottom="true"
                    android:layout_height="1dp" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/rl_category_a2"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1">

                <TextView
                    android:text="Salud"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/textView2"
                    android:textColor="@color/md_black_1000"
                    android:layout_centerHorizontal="true"
                    android:layout_below="@+id/imageView5" />

                <ImageView
                    android:layout_height="64dp"
                    app:srcCompat="@drawable/ic_local_hospital_black_24px"
                    android:id="@+id/imageView5"
                    android:layout_width="64dp"
                    android:layout_centerHorizontal="true"
                    android:layout_centerVertical="true"
                    tools:ignore="VectorDrawableCompat" />

                <ImageView
                    android:layout_height="match_parent"
                    app:srcCompat="@color/colorAccent"
                    android:id="@+id/imageView6"
                    android:layout_alignParentRight="true"
                    android:layout_width="1dp" />

                <ImageView
                    android:layout_width="match_parent"
                    app:srcCompat="@color/colorAccent"
                    android:id="@+id/imageView7"
                    android:layout_alignParentBottom="true"
                    android:layout_height="1dp" />

            </RelativeLayout>

        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1">

            <RelativeLayout
                android:id="@+id/rl_category_b1"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1">

                <TextView
                    android:text="Belleza"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/textView3"
                    android:textColor="@color/md_black_1000"
                    android:layout_centerHorizontal="true"
                    android:layout_below="@+id/imageView8" />

                <ImageView
                    android:layout_height="64dp"
                    app:srcCompat="@mipmap/belleza"
                    android:id="@+id/imageView8"
                    android:layout_width="64dp"
                    android:layout_centerHorizontal="true"
                    android:layout_centerVertical="true"
                    tools:ignore="VectorDrawableCompat" />

                <ImageView
                    android:layout_height="match_parent"
                    app:srcCompat="@color/colorAccent"
                    android:id="@+id/imageView9"
                    android:layout_alignParentRight="true"
                    android:layout_width="1dp" />

                <ImageView
                    android:layout_width="match_parent"
                    app:srcCompat="@color/colorAccent"
                    android:id="@+id/imageView10"
                    android:layout_alignParentBottom="true"
                    android:layout_height="1dp" />

            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/rl_category_b2"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1">

                <TextView
                    android:text="Comida"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/textView4"
                    android:textColor="@color/md_black_1000"
                    android:layout_centerHorizontal="true"
                    android:layout_below="@+id/imageView11" />

                <ImageView
                    android:layout_height="64dp"
                    app:srcCompat="@drawable/ic_restaurant_white_24px"
                    android:id="@+id/imageView11"
                    android:layout_width="64dp"
                    android:layout_centerHorizontal="true"
                    android:layout_centerVertical="true"
                    tools:ignore="VectorDrawableCompat" />

                <ImageView
                    android:layout_height="match_parent"
                    app:srcCompat="@color/colorAccent"
                    android:id="@+id/imageView12"
                    android:layout_alignParentRight="true"
                    android:layout_width="1dp" />

                <ImageView
                    android:layout_width="match_parent"
                    app:srcCompat="@color/colorAccent"
                    android:id="@+id/imageView13"
                    android:layout_alignParentBottom="true"
                    android:layout_height="1dp" />

            </RelativeLayout>
        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1">

            <RelativeLayout
                android:id="@+id/rl_category_c1"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1">

                <TextView
                    android:text="Nocturno"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/textView5"
                    android:textColor="@color/md_black_1000"
                    android:layout_centerHorizontal="true"
                    android:layout_below="@+id/imageView14" />

                <ImageView
                    android:layout_height="64dp"
                    app:srcCompat="@drawable/ic_local_bar_black_24px"
                    android:id="@+id/imageView14"
                    android:layout_width="64dp"
                    android:layout_centerHorizontal="true"
                    android:layout_centerVertical="true"
                    tools:ignore="VectorDrawableCompat" />

                <ImageView
                    android:layout_height="match_parent"
                    app:srcCompat="@color/colorAccent"
                    android:id="@+id/imageView15"
                    android:layout_alignParentRight="true"
                    android:layout_width="1dp" />

                <ImageView
                    android:layout_width="match_parent"
                    app:srcCompat="@color/colorAccent"
                    android:id="@+id/imageView16"
                    android:layout_alignParentBottom="true"
                    android:layout_height="1dp" />

            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/rl_category_c2"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1">

                <TextView
                    android:text="Varios"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/textView6"
                    android:textColor="@color/md_black_1000"
                    android:layout_centerHorizontal="true"
                    android:layout_below="@+id/imageView17" />

                <ImageView
                    android:layout_height="64dp"
                    app:srcCompat="@drawable/ic_local_library_white_24px"
                    android:id="@+id/imageView17"
                    android:layout_width="64dp"
                    android:layout_centerHorizontal="true"
                    android:layout_centerVertical="true"
                    tools:ignore="VectorDrawableCompat" />

                <ImageView
                    android:layout_height="match_parent"
                    app:srcCompat="@color/colorAccent"
                    android:id="@+id/imageView18"
                    android:layout_alignParentRight="true"
                    android:layout_width="1dp" />

                <ImageView
                    android:layout_width="match_parent"
                    app:srcCompat="@color/colorAccent"
                    android:id="@+id/imageView19"
                    android:layout_alignParentBottom="true"
                    android:layout_height="1dp" />

            </RelativeLayout>
        </LinearLayout>
    </LinearLayout>
</RelativeLayout>
